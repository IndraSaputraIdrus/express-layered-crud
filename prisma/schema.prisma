generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Product {
  id          Int    @id @default(autoincrement())
  name        String @db.VarChar(255)
  price       Int
  description String @db.VarChar(255)
  image       String @db.VarChar(255)
}
